{
    "name": "jsonld-graph",
    "version": "3.0.7",
    "description": "Library to parse, inspect and mutate JSON-LD documents as an in-memory graph.",
    "keywords": [
        "jsonld",
        "graph",
        "jsonld graph"
    ],
    "main": "./lib/index.js",
    "scripts": {
        "clean": "rimraf ./lib && rimraf ./coverage",
        "coverage": "rimraf ,/coverage && npm run test && opener ./coverage/index.html",
        "lint": "tslint --config tslint.json --project tsconfig.json --exclude '**/*.d.ts' src/**/*",
        "pretty": "prettier --config .prettierrc.yml --log=silent --write \"src/**/*.ts\"",
        "build": "npm run pretty && npm run lint  && tsc",
        "build-dist": "npm run clean && npm run build && npm run test",
        "prepublishOnly": "npm run build-dist",
        "test": "nyc mocha --opts mocha.spec.opts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/riteshrao/jsonld-graph.git"
    },
    "author": "Ritesh Rao",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/riteshrao/jsonld-graph/issues"
    },
    "homepage": "https://github.com/riteshrao/jsonld-graph#readme",
    "devDependencies": {
        "@types/chai": "^4.2.3",
        "@types/debug": "^4.1.5",
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.7.12",
        "@types/shortid": "0.0.29",
        "chai": "^4.2.0",
        "mocha": "^6.2.1",
        "nyc": "^14.1.1",
        "opener": "^1.5.1",
        "prettier": "^1.18.2",
        "rimraf": "^3.0.0",
        "source-map-support": "^0.5.13",
        "ts-node": "^8.4.1",
        "tslint": "^5.20.0",
        "tslint-config-airbnb": "^5.11.2",
        "tslint-config-prettier": "^1.18.0",
        "tslint-no-unused-expression-chai": "^0.1.4",
        "typescript": "^3.6.4"
    },
    "dependencies": {
        "events": "^3.0.0",
        "jsiterable": "^3.0.0",
        "jsonld": "^1.8.0",
        "lodash.clonedeep": "^4.5.0",
        "shortid": "^2.2.15",
        "uri-js": "^4.2.2"
    },
    "nyc": {
        "all": true,
        "cache": false,
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "**/*.d.ts",
            "test/**",
            "sample/**"
        ],
        "reporter": [
            "html",
            "json",
            "text-summary"
        ],
        "extension": [
            ".ts"
        ],
        "source-map": true,
        "instrument": true
    }
}