{
  "name": "jsonld-graph",
  "version": "2.0.3",
  "description": "Library to parse, inspect and mutate JSON-LD documents as an in-memory graph.",
  "keywords": [
    "jsonld",
    "graph",
    "jsonld graph"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf ./lib && rimraf ./coverage",
    "coverage": "rimraf ,/coverage && npm run test && opener ./coverage/index.html",
    "build": "tsc && cp src/eventEmitter.d.ts lib",
    "build-dist": "npm run clean && npm run build && npm run test",
    "prepublishOnly": "npm run build-dist",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riteshrao/jsonld-graph.git"
  },
  "author": "Ritesh Rao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/riteshrao/jsonld-graph/issues"
  },
  "homepage": "https://github.com/riteshrao/jsonld-graph#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "opener": "^1.5.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "@types/node": "^11.10.0",
    "@types/shortid": "0.0.29",
    "events": "^3.0.0",
    "jsiterable": "^1.0.2",
    "jsonld": "^1.5.4",
    "lodash.clonedeep": "^4.5.0",
    "shortid": "^2.2.14",
    "uri-js": "^4.2.2"
  },
  "nyc": {
    "all": true,
    "cache": false,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**",
      "sample/**"
    ],
    "reporter": [
      "html",
      "json",
      "text-summary"
    ],
    "extension": [
      ".ts"
    ],
    "source-map": true,
    "instrument": true
  }
}
